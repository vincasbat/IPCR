<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="lt.vinco.ebdimport.entity.Represent" table="represent">
      
      <composite-id name="id" class="lt.vinco.ebdimport.entity.RepresentId">
            <key-property name="idappli" type="string">
                <column name="idappli" length="12" />
            </key-property>
             <key-property name="idagent" type="java.lang.Short">
                <column name="idagent" />
            </key-property>
        </composite-id>
       
  <property name="odagent" type="java.lang.Short">   <column name="odagent"/>  </property>
  <property name="locked" type="java.lang.Short">   <column name="locked"/>  </property>
      
      
  </class>
</hibernate-mapping>
